{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aula-anime": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/aula-anime",
      "sourceRoot": "apps/aula-anime/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/aula-anime",
            "index": "apps/aula-anime/src/index.html",
            "main": "apps/aula-anime/src/main.ts",
            "polyfills": "apps/aula-anime/src/polyfills.ts",
            "tsConfig": "apps/aula-anime/tsconfig.app.json",
            "assets": [
              "apps/aula-anime/src/favicon.ico",
              "apps/aula-anime/src/assets"
            ],
            "styles": [
              "apps/aula-anime/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4200
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/aula-anime/src/test.ts",
            "polyfills": "apps/aula-anime/src/polyfills.ts",
            "tsConfig": "apps/aula-anime/tsconfig.spec.json"
          }
        }
      }
    },
